{
    "name": "@thanxngo/redux-crudo",
    "version": "0.1.3",
    "description": "A package for writing convention-driven reducers and actions for Redux.",
    "main": "index.js",
    "scripts": {
        "lint": "eslint src/",
        "test": "jest --coverage '--collectCoverageFrom=src/**/*.js'",
        "coveralls": "jest --coverage  '--collectCoverageFrom=src/**/*.js' && cat ./coverage/lcov.info | coveralls",
        "compile": "babel -d lib/ src/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/thanxngo/redux-crudo.git"
    },
    "author": "Thanxngo Dev Team <dev@thanxngo.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/thanxngo/redux-crudo/issues"
    },
    "homepage": "https://github.com/thanxngo/redux-crudo/#README",
    "devDependencies": {
        "babel": "6.23.0",
        "babel-cli": "6.26.0",
        "babel-core": "6.26.3",
        "babel-eslint": "10.1.0",
        "babel-jest": "25.1.0",
        "babel-plugin-root-import": "6.5.0",
        "babel-plugin-transform-object-rest-spread": "6.26.0",
        "babel-preset-env": "1.7.0",
        "coveralls": "3.0.11",
        "eslint": "6.8.0",
        "eslint-config-airbnb": "18.1.0",
        "eslint-plugin-import": "2.20.1",
        "eslint-plugin-jsx-a11y": "6.2.3",
        "eslint-plugin-react": "7.19.0",
        "jest": "25.1.0",
        "prettier": "1.19.1"
    },
    "dependencies": {
        "immutability-helper": "^3.0.0",
        "immutable": "^3.8.2",
        "prop-types": "^15.6.2"
    },
    "prettier": {
        "tabWidth": 4,
        "trailingComma": "es5"
    },
    "jest": {
        "collectCoverage": true,
        "collectCoverageFrom": [
            "src/**/*.js"
        ],
        "coverageReporters": [
            "json",
            "html"
        ],
        "transform": {
            "^.+\\.jsx?$": "babel-jest"
        }
    },
    "directories": {
        "lib": "lib"
    }
}
